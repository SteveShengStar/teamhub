    "dotenv": "^10.0.0",
    "eslint-plugin-react": "^7.16.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "googleapis": "^44.0.0",
    "mongodb": "^3.6.2",
    "mongoose": "^5.12.7",
    "mongoose-deep-populate": "^3.2.0",
    "mongoose-unique-validator": "^2.0.3",
    "next": "^10.2.3",
    "next-redux-wrapper": "^4.0.1",
    "nodemon": "^1.19.3",
    "now": "^16.6.0",
