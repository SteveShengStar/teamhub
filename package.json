{
  "name": "teamhub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./server.js",
    "build": "next build",
    "build:docker": "docker build ./docker -t teamhub",
    "start:docker": "docker-compose up -d",
    "dev:docker": "docker exec -it teamhub_nodejs npm run dev",
    "docker": "npm run start:docker && npm run dev:docker",
    "start": "next start",
    "test": "TEAMHUB_ENV=testing next build && TEAMHUB_ENV=testing jest"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "dependencies": {
    "animejs": "^3.2.1",
    "axios": "^0.22.0",
    "eslint-plugin-react": "^7.26.1",
    "express": "^4.17.1",
    "google-auth-library": "^7.10.0",
    "googleapis": "^88.2.0",
    "lodash": "^4.17.21",
    "mongodb": "^4.1.3",
    "mongoose": "^6.0.10",
    "mongoose-deep-populate": "^3.2.0",
    "mongoose-unique-validator": "^3.0.0",
    "next": "^11.1.2",
    "nodemon": "^2.0.13",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
    "react-redux": "^7.2.5",
    "react-select": "^5.1.0",
    "redux": "^4.1.1",
    "redux-persist": "^6.0.0",
    "styled-components": "^5.3.1",
    "styled-system": "^5.1.5",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "babel-jest": "^27.2.5",
    "babel-plugin-styled-components": "^1.13.2",
    "jest": "^27.2.5",
    "redux-devtools-extension": "^2.13.9",
    "supertest": "^6.1.6"
  }
